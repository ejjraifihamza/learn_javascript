let job = 'manager'
salary = 0
switch(job){
    case (job = 'devloper') :
        salary = 9000
        console.log(salary);
        break
    case ((job = 'manager'), (job = 'designer')) :
        salary = 8000
        console.log(salary);
        break
    default :
    salary = 0
    console.log(salary);
}